{
  "require": {
    "andreas83/craft-countryselect": "^1.2",
    "craftcms/cms": "3.5.11.1",
    "craftcms/redactor": "2.7.4",
    "ether/simplemap": "3.8.4.1",
    "focuslabllc/craft-cheat-sheet": "2.0.3",
    "mmikkel/retcon": "^2.2",
    "sebastianlenz/linkfield": "1.0.23",
    "vlucas/phpdotenv": "^3.4.0",
    "wrav/simplesharing": "1.0.6",
    "youandmedigital/breadcrumb": "1.1.0"
  },
  "require-dev": {
    "yiisoft/yii2-shell": "^2.0.3"
  },
  "autoload": {
    "psr-4": {
      "modules\\": "modules/"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "platform": {
      "php": "7.0"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ]
  }
}
